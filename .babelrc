{
  "extends": "react-static/.babelrc",
  "plugins": [["import", {"libraryName": "antd", "style": true}]],
  "env": {
    "production": {
      "plugins": [
        ["emotion", {"sourceMap": false, "hoist": true, "autoLabel": true}]
      ]
    },
    "development": {
      "plugins": [
        ["emotion", {"sourceMap": true, "hoist": true, "autoLabel": true}]
      ]
    }
  }
}
